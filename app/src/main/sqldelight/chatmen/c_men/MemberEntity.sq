CREATE TABLE MemberEntity (
    username TEXT PRIMARY KEY,
    name TEXT NOT NULL,
    bio TEXT,
    profilePictureUrl TEXT
);

getAllMembers:
SELECT *
FROM MemberEntity
ORDER BY name ASC;

getMemberByUsername:
SELECT *
FROM MemberEntity
WHERE username = :username;

insert:
INSERT OR REPLACE
INTO MemberEntity
VALUES (?, ?, ?, ?);

deleteAll:
DELETE FROM MemberEntity;