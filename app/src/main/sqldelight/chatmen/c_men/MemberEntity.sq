CREATE TABLE MemberEntity (
    username TEXT PRIMARY KEY,
    bio TEXT,
    profilePictureUrl TEXT,
    isOwnProfile INTEGER AS Boolean NOT NULL DEFAULT 0
);

getAllMembers:
SELECT *
FROM MemberEntity
ORDER BY username ASC;

getMemberByUsername:
SELECT *
FROM MemberEntity
WHERE username = :username;

insert:
INSERT OR REPLACE
INTO MemberEntity
VALUES (?, ?, ?, ?);

deleteAll:
DELETE FROM MemberEntity;